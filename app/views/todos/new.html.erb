<h1>New List Items</h1>
<%= form_for @todo do |f| %>


  <%= f.label :item %>
  <%= f.text_field :item, placeholder: 'List Item' %>
  <br/>

  <%= f.label :completed %>
  <%= f.text_field :completed, placeholder: 'True/False' %>
  <br/>

  <%= f.label :date %>
  <%= f.text_field :date, placeholder: 'Date'%>
  <br/>

  <%= f.submit %>

<% end %>